{
  "name": "@svgtvi/core",
  "version": "0.1.0-alpha.0",
  "author": "dong.xing@outlook.com",
  "description": "transform svgs to vue component",
  "homepage": "https://github.com/WX-DongXing/svgtvi",
  "repository": "https://github.com/WX-DongXing/svgtvi",
  "license": "MIT",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "tsnd --respawn src/index.ts --ignore-watch dist",
    "build": "tsup"
  },
  "keywords": [
    "svg",
    "vue",
    "icon",
    "vue3",
    "svg-to-vue",
    "svg-to-vue-icon"
  ],
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "dependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@svgtvi/plugin-preview": "workspace:*",
    "@vue/compiler-sfc": "^3.2.37",
    "change-case": "^4.1.2",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/svgo": "^2.6.3"
  }
}
