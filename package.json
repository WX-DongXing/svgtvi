{
  "name": "svgtvc",
  "version": "0.1.0",
  "author": "dong.xing@outlook.com",
  "description": "transform svgs to vue component",
  "homepage": "https://github.com/WX-DongXing/svgtvc",
  "repository": "https://github.com/WX-DongXing/svgtvc",
  "license": "MIT",
  "type": "commonjs",
  "bin": {
    "svgtvc": "lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "tsnd --respawn src/index.ts --ignore-watch dist",
    "build": "tsup"
  },
  "keywords": [
    "svg",
    "vue",
    "vue3",
    "svg-to-vue",
    "svg-to-vue-component"
  ],
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@vue/compiler-sfc": "^3.2.37",
    "change-case": "^4.1.2",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.3",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  }
}
